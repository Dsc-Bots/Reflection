{
	"sourceMaps": "inline",
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true,
		"noInterop": false
	},
	"jsc": {
		"externalHelpers": false,
		"target": "es2016",
		"loose": true,
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": false,
			"dynamicImport": true
		},
		"minify": {
			"compress": false,
			"mangle": false
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"keepClassNames": true
	},
	"minify": true
}
